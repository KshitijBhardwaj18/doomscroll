{
  "expo": {
    "name": "Doomscroll",
    "slug": "doomscroll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "doomscroll",
    "newArchEnabled": "true",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doomscroll.app",
      "icon": "./assets/logo.png",
      "associatedDomains": [
        "applinks:doomscroll.com",
        "applinks:www.doomscroll.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.doomscroll.solana",
      "icon": "./assets/logo.png",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "doomscroll.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.doomscroll.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "6dccc9f9-1446-415c-9fcb-7273622dce30"
      }
    }
  }
}
